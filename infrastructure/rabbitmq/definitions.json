{
  "rabbit_version": "3.12.0",
  "rabbitmq_version": "3.12.0",
  "product_name": "RabbitMQ",
  "product_version": "3.12.0",
  "users": [
    {
      "name": "admin",
      "password_hash": "admin123",
      "hashing_algorithm": "rabbit_password_hashing_sha256",
      "tags": "administrator",
      "limits": {}
    },
    {
      "name": "ivyarc_user",
      "password_hash": "ivyarc_password",
      "hashing_algorithm": "rabbit_password_hashing_sha256",
      "tags": "management",
      "limits": {}
    }
  ],
  "vhosts": [
    {
      "name": "ivyarc",
      "description": "IvyArc microservices virtual host",
      "tags": [],
      "default_queue_type": "classic",
      "metadata": {
        "description": "Virtual host for IvyArc Spring Cloud microservices",
        "tags": []
      }
    }
  ],
  "permissions": [
    {
      "user": "admin",
      "vhost": "ivyarc",
      "configure": ".*",
      "write": ".*",
      "read": ".*"
    },
    {
      "user": "ivyarc_user",
      "vhost": "ivyarc",
      "configure": ".*",
      "write": ".*",
      "read": ".*"
    }
  ],
  "topic_permissions": [],
  "parameters": [],
  "global_parameters": [
    {
      "name": "internal_cluster_id",
      "value": "rabbitmq-cluster-id-ivyarc"
    }
  ],
  "policies": [
    {
      "vhost": "ivyarc",
      "name": "ha-policy",
      "pattern": ".*",
      "apply-to": "queues",
      "definition": {
        "ha-mode": "exactly",
        "ha-params": 2,
        "ha-sync-mode": "automatic",
        "message-ttl": 86400000
      },
      "priority": 0
    },
    {
      "vhost": "ivyarc",
      "name": "dlx-policy",
      "pattern": ".*\.dlq",
      "apply-to": "queues",
      "definition": {
        "message-ttl": 604800000,
        "max-length": 10000
      },
      "priority": 1
    }
  ],
  "queues": [
    {
      "name": "auth.events",
      "vhost": "ivyarc",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 86400000,
        "x-dead-letter-exchange": "auth.dlx",
        "x-dead-letter-routing-key": "auth.events.dlq"
      }
    },
    {
      "name": "auth.events.dlq",
      "vhost": "ivyarc",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 604800000
      }
    },
    {
      "name": "authorization.events",
      "vhost": "ivyarc",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 86400000,
        "x-dead-letter-exchange": "authorization.dlx",
        "x-dead-letter-routing-key": "authorization.events.dlq"
      }
    },
    {
      "name": "authorization.events.dlq",
      "vhost": "ivyarc",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 604800000
      }
    },
    {
      "name": "user.events",
      "vhost": "ivyarc",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 86400000,
        "x-dead-letter-exchange": "user.dlx",
        "x-dead-letter-routing-key": "user.events.dlq"
      }
    },
    {
      "name": "user.events.dlq",
      "vhost": "ivyarc",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 604800000
      }
    },
    {
      "name": "audit.events",
      "vhost": "ivyarc",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 2592000000,
        "x-dead-letter-exchange": "audit.dlx",
        "x-dead-letter-routing-key": "audit.events.dlq"
      }
    },
    {
      "name": "audit.events.dlq",
      "vhost": "ivyarc",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 2592000000
      }
    }
  ],
  "exchanges": [
    {
      "name": "auth.exchange",
      "vhost": "ivyarc",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    },
    {
      "name": "auth.dlx",
      "vhost": "ivyarc",
      "type": "direct",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    },
    {
      "name": "authorization.exchange",
      "vhost": "ivyarc",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    },
    {
      "name": "authorization.dlx",
      "vhost": "ivyarc",
      "type": "direct",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    },
    {
      "name": "user.exchange",
      "vhost": "ivyarc",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    },
    {
      "name": "user.dlx",
      "vhost": "ivyarc",
      "type": "direct",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    },
    {
      "name": "audit.exchange",
      "vhost": "ivyarc",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    },
    {
      "name": "audit.dlx",
      "vhost": "ivyarc",
      "type": "direct",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    }
  ],
  "bindings": [
    {
      "source": "auth.exchange",
      "vhost": "ivyarc",
      "destination": "auth.events",
      "destination_type": "queue",
      "routing_key": "auth.*",
      "arguments": {}
    },
    {
      "source": "auth.dlx",
      "vhost": "ivyarc",
      "destination": "auth.events.dlq",
      "destination_type": "queue",
      "routing_key": "auth.events.dlq",
      "arguments": {}
    },
    {
      "source": "authorization.exchange",
      "vhost": "ivyarc",
      "destination": "authorization.events",
      "destination_type": "queue",
      "routing_key": "authorization.*",
      "arguments": {}
    },
    {
      "source": "authorization.dlx",
      "vhost": "ivyarc",
      "destination": "authorization.events.dlq",
      "destination_type": "queue",
      "routing_key": "authorization.events.dlq",
      "arguments": {}
    },
    {
      "source": "user.exchange",
      "vhost": "ivyarc",
      "destination": "user.events",
      "destination_type": "queue",
      "routing_key": "user.*",
      "arguments": {}
    },
    {
      "source": "user.dlx",
      "vhost": "ivyarc",
      "destination": "user.events.dlq",
      "destination_type": "queue",
      "routing_key": "user.events.dlq",
      "arguments": {}
    },
    {
      "source": "audit.exchange",
      "vhost": "ivyarc",
      "destination": "audit.events",
      "destination_type": "queue",
      "routing_key": "audit.*",
      "arguments": {}
    },
    {
      "source": "audit.dlx",
      "vhost": "ivyarc",
      "destination": "audit.events.dlq",
      "destination_type": "queue",
      "routing_key": "audit.events.dlq",
      "arguments": {}
    }
  ]
}